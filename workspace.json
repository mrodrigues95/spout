{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/next",
    "packageManager": "npm"
  },
  "defaultProject": "web",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "api": "apps/api",
    "shared-assets": "libs/shared/assets",
    "shared-utils": "libs/shared/utils",
    "toolkit": "libs/toolkit",
    "web": "apps/web"
  }
}

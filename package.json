    "date-fns": "^2.22.1",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.0",
    "iron-session": "^6.0.4",
    "lexical": "^0.2.0",
    "libphonenumber-js": "^1.9.50",
    "next": "^12.1.0",
    "next-seo": "^4.24.0",

{
  "name": "spout",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mrodrigues95/spout.git"
  },
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "bootstrap": "yarn --ignore-platform",
    "add-project": "nx g @nrwl/workspace:library",
    "rename-project": "nx g @nrwl/workspace:mv --project {args.src} --destination {args.dest}",
    "remove-project": "nx g @nrwl/workspace:remove {args.name}",
    "dev:up": "docker compose --file .dev/docker-compose.yml up",
    "dev:down": "docker compose --file .dev/docker-compose.yml down",
    "dev:setup:localhost": "node .dev/scripts/elevate.js .dev/scripts/setupLocalhost.js",
    "dev:setup:https": "node .dev/scripts/setupHttps.js",
    "check:lint": "nx run-many --target=lint --all",
    "check:ts-check": "nx run-many --target=ts-check --all",
    "update": "nx migrate latest",
    "migrate": "nx migrate --run-migrations",
    "prepare": "husky install && nx g @nx-dotnet/core:restore"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.8.0",
